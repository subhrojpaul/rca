            minDate : moment()
        });
        $('input[name="travel_date"]').on('apply.daterangepicker', function(ev, picker) {
          $(this).val(picker.startDate.format('DD/MM/YYYY'));
        });

        $('input[name="travel_date"]').on('cancel.daterangepicker', function(ev, picker) {
          $(this).val('');
        });
    });
    </script>
</body>

</html>
