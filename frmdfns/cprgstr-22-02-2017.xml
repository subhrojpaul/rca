			<name>cprgstr_pswd1</name>
			<label>Password</label>
			<size>large</size>
			<mandatory>yes</mandatory>
			<additional>Password Requirements: 8-20 chars, min 1 lowercase letter, 1 uppercase letter and 1 number</additional>
		</element>
		<element>
			<type>password</type>
			<name>cprgstr_pswd2</name>
			<label>Confirm Password</label>
			<size>large</size>
			<mandatory>yes</mandatory>
		</element>

		<element>
			<type>dynamic_select_with_value</type>
			<name>cprgstr_agent_id</name>
			<select_stmt>select -99 agent_id, "select one.." agent_name from dual union all select agent_id, agent_name from rca.agents</select_stmt>
			<label>Agent</label>
			<size>large</size>
			<mandatory>yes</mandatory>
		</element>
		
		
